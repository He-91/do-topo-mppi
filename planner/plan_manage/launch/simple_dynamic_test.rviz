Panels:
  - Class: rviz/Displays
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /DynamicObstacles1
      Splitter Ratio: 0.5
    Tree Height: 565
  - Class: rviz/Views
    Name: Views
    Splitter Ratio: 0.5
  - Class: rviz/Time
    Name: Time

Visualization Manager:
  Class: ""
  Displays:
    - Alpha: 0.5
      Cell Size: 1
      Class: rviz/Grid
      Color: 160; 160; 164
      Enabled: true
      Line Style:
        Line Width: 0.03
        Value: Lines
      Name: Grid
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 100
      Reference Frame: world
      Value: true

    # 动态障碍物点云
    - Alpha: 0.8
      Autocompute Intensity Bounds: true
      Class: rviz/PointCloud2
      Color: 255; 0; 0
      Color Transformer: FlatColor
      Enabled: true
      Name: DynamicObstaclesCloud
      Position Transformer: XYZ
      Size (Pixels): 3
      Size (m): 0.08
      Style: Points
      Topic: /dynamic_obstacles/cloud
      Value: true

    # 动态障碍物标记
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /dynamic_obstacles/markers
      Name: DynamicObstacles
      Value: true

    # 障碍物速度向量
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /dynamic_obstacles/velocities
      Name: ObstacleVelocities
      Value: true

  Enabled: true
  Global Options:
    Background Color: 48; 48; 48
    Default Light: true
    Fixed Frame: world
    Frame Rate: 30
  Name: root
  Tools:
    - Class: rviz/Interact
    - Class: rviz/MoveCamera
    - Class: rviz/Select
  Value: true
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 35
      Enable Stereo Rendering:
        Value: false
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 0.785398
      Target Frame: world
      Value: Orbit (rviz)
      Yaw: 0.785398

Window Geometry:
  Displays:
    collapsed: false
  Height: 846
  Width: 1200
  X: 100
  Y: 100
